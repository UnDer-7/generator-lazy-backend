NODE_ENV=development
PORT=3000
<%if (answer.login) { %>
APP_SECRET=<%= appSecret %>
<%_ } _%>
<% if (answer.databaseStyle === 'mongo') { %>
DB_URL=mongodb://localhost:27017/<%= answer.databaseName %>
<%_ } _%>
<% if (answer.databaseStyle === 'sql') { %>
# ---DATABASE SETTINGS---
USER_NAME=<%= answer.username%>
USER_PASSWORD=<%= answer.password%>
DATABASE=<%= answer.databaseName%>
HOST=localhost
DIALECT=<%= answer.sqlDB%>
DATABASE_PORT=<%= answer.dbPort%>
<%_}_%>
